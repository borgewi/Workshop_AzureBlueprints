{
    "properties": {
        "description": "Blueprintdefinisjon for utviklingsmilj√∏",
        "targetScope": "subscription",
        "parameters": { 
            "IN_principalId": {
                "type": "String",
                "metadata": {
                    "description": "Security principal, se https://docs.microsoft.com/en-us/azure/role-based-access-control/overview",
                    "strongType": "PrincipalId"
                }
            },
            "IN_storageAccountName": {
                "type": "String"
            },
            "IN_virtualMachineName": {
                "type": "String"
            },
            "IN_networkInterfaceName": {
                "type": "String"
            },
            "IN_pipName": {
                "type": "String"
            },
            "IN_vnetName": {
                "type": "String"
            },
            "IN_vmUsername": {
                "type": "String"
            },
            "IN_vmPass": {
                "type": "String",
                "metadata":{
                    "description": "Password for the Virtual Machine"
                }
            },
            "IN_location": {
                "defaultValue": "northeurope",
                "type": "String"
            }
        },
        "resourceGroups": {
            "Blueprint-RG": {
                "location": "[parameters('IN_location')]"
            }
        }
    },
    "type": "Microsoft.Blueprint/blueprints" 
}