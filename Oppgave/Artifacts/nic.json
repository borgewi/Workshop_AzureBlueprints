{
    "kind": "template",
    "properties": {
        "template": {
            "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
            "contentVersion": "1.0.0.0",
            "parameters": {
                "pipName": {
                    "type": "string"
                },
                "vnetName": {
                    "type": "string"
                },
                "networkInterfacesName": {
                    "type": "string"
                },
                "location": {
                    "type": "string"
                }
            },
            "variables": {
                "subnetName": "Subnet",
                "subnetPrefix": "10.0.0.0/24')]",
                "subnetRef": "[resourceId('Microsoft.Network/virtualNetworks/subnets', parameters('vnetName'), variables('subnetName'))]"
              },
            "resources": [
            {
                "type": "Microsoft.Network/networkInterfaces",
                "apiVersion": "2019-04-01",
                "name": "[parameters('networkInterfacesName')]",
                "location": "[parameters('location')]",
                "properties": {
                    "ipConfigurations": [
                        {
                            "name": "ipconfig1",
                            "etag": "W/\"47bc78c0-439c-413b-98e2-a9939bc5936f\"",
                            "properties": {
                                "provisioningState": "Succeeded",
                                "privateIPAddress": "10.0.0.4",
                                "privateIPAllocationMethod": "Dynamic",
                                "publicIPAddress": {
                                    "id": "[resourceId('Microsoft.Network/publicIPAddresses', parameters('pipName'))]"
                                },
                                "subnet": {
                                    "id": "[variables('subnetRef')]"
                                },
                                "primary": true,
                                "privateIPAddressVersion": "IPv4"
                            }
                        }
                    ]
                }
            }
        ]
        },
        "resourceGroup": "Blueprint-RG",
        "parameters": {
            "pipName": {
                "value": "[parameters('BP_pipName')]"
            },
            "vnetName": {
                "value": "[parameters('BP_vnetName')]"
            },
            "networkInterfacesName": {
                "value": "[parameters('BP_networkInterfaceName')]"
            },
            "location": {
                "value": "[parameters('BP_location')]"
            }
        },
        "dependsOn": [
            "pip",
            "vnet"
        ]
    },
    "type": "Microsoft.Blueprint/blueprints/artifacts"
}